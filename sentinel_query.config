# simple config file to find and download data found at the ASF archive

# API search options
# Note: do not use the keyword 'output' as this will be set to 'csv' by the code.
[api_search]

# search options here
platform = Sentinel-1A,Sentinel-1B
processingLevel = SLC
beamMode = IW
# point location: Singapore
intersectsWith = point(103.8 60.3)
start=2019-04-01T00:00:00UTC
end=2019-05-01T00:00:00UTC
#relativeOrbit=18

# download_site may be AWS, ASF, or both (in this case, tries AWS first)
[download]
download_site = both
aws_base_url = http://sentinel1-slc-seasia-pds.s3-website-ap-southeast-1.amazonaws.com/datasets/slc/v1.1/

# ASF requires fields http-user and http-password for wget
[asf_download]
http-user = 
http-password = 

